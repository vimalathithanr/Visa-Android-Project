<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="270dp"
    android:layout_height="240dp"
    android:padding="15dp"
    android:background="@drawable/white_background_shadow">

    <TextView
        android:id="@+id/dialog_username_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="5dp"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="16sp"
        android:text="@string/dialog_username_title"/>

    <TextView
        android:id="@+id/text"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="@string/dialog_username_remember_text"
        android:textColor="@color/label_hint_color"
        android:layout_below="@+id/dialog_username_title"/>

    <TextView
        android:id="@+id/dialogButtonOK"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/dialog_username_button_text"
        android:layout_marginTop="25dp"
        android:textColor="@color/label_hint_color"
        android:layout_below="@+id/text" />

