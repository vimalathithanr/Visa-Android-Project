<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_terms_privacy_container_rc"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/rlLinkContainer_rc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:gravity="center">

        <RelativeLayout
            android:id="@+id/rlLinkContainer_rc_minus_divider"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:gravity="center_horizontal">

            <ImageView
                android:id="@+id/iv_terms_privacy_lock_rc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginBottom="@dimen/thirty_px"
                android:layout_marginLeft="@dimen/layout_margin_left_right"
                android:layout_marginTop="@dimen/thirty_px"
                android:adjustViewBounds="true"
                android:contentDescription="@string/accessibility_secure_lock"
                android:maxHeight="@dimen/pl_forty_five_px"
                android:maxWidth="@dimen/pl_thirty_five_px"
                android:minHeight="@dimen/pl_forty_five_px"
                android:minWidth="@dimen/pl_thirty_five_px"
                android:scaleType="fitXY"
                android:src="@drawable/ic_lock" />

            <TextView
                android:id="@+id/tv_terms_link_rc"
                style="@style/VisaFooterLinkTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/iv_terms_privacy_lock_rc"
                android:contentDescription="@string/tv_terms_content_description"
                android:text="@string/tv_terms" />

            <TextView
                android:id="@+id/tv_privacy_link_rc"
                style="@style/VisaFooterLinkTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/tv_terms_link_rc"
                android:contentDescription="@string/tv_privacy_content_description"
                android:text="@string/tv_privacy" />
        </RelativeLayout>


        <View
            android:id="@+id/v_footer_divider_rc"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/footer_divider_width"
            android:layout_alignParentTop="true"
            android:background="@color/footer_divider_color" />

        <View
            android:id="@+id/v_footer_divider_rc_bottom"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/footer_divider_width"
            android:layout_below="@id/rlLinkContainer_rc_minus_divider"
            android:background="@color/footer_divider_color" />

        <TextView
            android:id="@+id/tv_invisible_view_rc"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/v_footer_divider_rc_bottom"
            android:layout_marginTop="@dimen/sixty_px" />

    </RelativeLayout>

